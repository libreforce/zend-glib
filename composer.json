{
    "name": "libreforce/zend-glib",
    "description": "provides tools for working with GLib",
    "type": "library",
    "require": {
        "php": ">=7.2.6",
        "zendframework/zend-loader": "2.6.*@dev",
        "zendframework/zend-filter": "2.8.*@dev",
        "zendframework/zend-servicemanager": "3.3.*@dev",
        "zendframework/zend-view": "2.10.*@dev",
        "zendframework/zend-stdlib": "3.2.*@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.2@dev"
    },
    "autoload": {
        "psr-4": {
            "Zend\\GLib\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ZendTest\\GLib\\": "test/"
        }
    },
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Fred",
            "email": "morrisfrederique152@gmail.com"
        }
    ],
    "minimum-stability": "dev"
}